<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0"
	xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
	<persistence-unit name="cmd">
		<jta-data-source>java:/OracleMsMac</jta-data-source>
		<class>mac.entidades.Grupo</class>
		<class>mac.entidades.Concepto</class>
		<class>mac.entidades.Centros</class>
		<class>mac.entidades.CapacidadBodega</class>
		<class>mac.entidades.ModelosAliquidar</class>
		<class>mac.entidades.ClusterProducto</class>
		<class>mac.entidades.DetalleProductoLiqui</class>
		<class>mac.entidades.ViewModAliqUsuarios</class>
		<class>mac.entidades.LiquidacionesTienda</class>
		<properties>
			<property name="hibernate.dialect"
				value="org.hibernate.dialect.Oracle10gDialect" />
			<property name="hibernate.show_sql" value="true" />
			<property name="hibernate.format_sql" value="true" />
		</properties>
	</persistence-unit>
</persistence>
